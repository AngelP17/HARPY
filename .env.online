# HARPY backend preset: online mode (real provider adapters)
ENABLE_REAL_ADSB=true
ENABLE_REAL_TLE=true
ENABLE_REAL_SEISMIC=true
ENABLE_REAL_WEATHER_NWS=true
ENABLE_REAL_RADAR_NEXRAD=true

# OpenSky: leave credentials empty for anonymous mode, or set for higher quota
OPENSKY_CLIENT_ID=
OPENSKY_CLIENT_SECRET=
OPENSKY_ANON_MIN_INTERVAL_SECS=300

# Conservative polling defaults to respect upstream limits
ADSB_POLL_INTERVAL_SECS=300
TLE_POLL_INTERVAL_SECS=7200
SEISMIC_POLL_INTERVAL_SECS=300
WEATHER_POLL_INTERVAL_SECS=300
NEXRAD_POLL_INTERVAL_SECS=300
